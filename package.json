{
	"name": "template",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"type-check": "tsc --project tsconfig.json --pretty --noEmit",
		"lint": "next lint --ignore-path .gitignore --cache --fix",
		"format": "prettier --ignore-path .gitignore \"src/**/*.{js,ts,jsx,tsx}\" --write --cache",
		"prepare": "husky install"
	},
	"lint-staged": {
		"./src/**/*.{ts,js,jsx,tsx}": [
			"npm run lint",
			"npm run type-check",
			"npm run format"
		]
	},
	"dependencies": {
		"@formatjs/intl-localematcher": "^0.5.4",
		"@hookform/resolvers": "^3.3.4",
		"clsx": "^2.1.0",
		"husky": "^9.0.11",
		"negotiator": "^0.6.3",
		"next": "14.1.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.51.2",
		"tailwind-merge": "^2.2.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/negotiator": "^0.6.3",
		"@types/node": "20.12.4",
		"@types/react": "18.2.74",
		"@types/react-dom": "18.2.24",
		"autoprefixer": "^10.4.19",
		"class-variance-authority": "^0.7.0",
		"css-loader": "^7.0.0",
		"daisyui": "^4.10.0",
		"eslint": "8.57.0",
		"eslint-config-next": "14.1.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.3",
		"typescript": "5.4.4"
	}
}
